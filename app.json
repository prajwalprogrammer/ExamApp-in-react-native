{
  "expo": {
    "name": "ExamDemo",
    "slug": "ExamDemo",
    "version": "1.0.0",
    "orientation": "portrait",
    "githubUrl":"https://github.com/prajwalprogrammer/ExamApp-in-react-native",
    "icon": "./assets/logo2.png",
    "androidStatusBar":{
      "backgroundColor":"#336699"
    },
    "splash": {
      "image": "./assets/logo2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.operand.ExamDemo",
      "versionCode": 1,
      "enableDangerousExperimentalLeanBuilds":true
    },

    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}